{
  "builds": [
    {
      "src": "backend/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/app.py"
    }
  ],
  "functions": {
    "backend/app.py": {
      "runtime": "python3.9"
    }
  },
  "env": {
    "PYTHONPATH": "./backend"
  }
}
